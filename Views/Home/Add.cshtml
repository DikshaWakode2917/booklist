@model BooksListitingApis.Models.Books

@{
    
}
    <form method="post" action="Add">
     <div class="form-group">
         <label for="">Id</label>
         <input type="Number" class="form-control" asp-for="Id">
        
    </div>
     <div class="form-group">
         <label for="">Title</label>
         <input type="text" class="form-control" asp-for="Title">
        
    </div>
     <div class="form-group">
         <label for="">Author</label>
         <input type="text" class="form-control" asp-for="Author">
        
    </div>
     <div class="form-group">
         <label for="">ISBN</label>
         <input type="text" class="form-control" asp-for="ISBN">   
    </div>
     <div class="form-check">
         <input type="checkbox" class="form-check-input" id="exampleCheck1">
         <label class="form-check-label" for="exampleCheck1">Check </label>
        
    </div>
     <button type="submit" class="btn btn-primary">Submit</button>
   
</form>



